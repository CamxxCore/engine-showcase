  - !ruby/object:GraphicsComponent 
    model: /data/entities/models/marine.model
    system: graphics
    type: default
  - !ruby/object:PhysicsComponent 
    body: /data/entities/bodies/player.hkx
    system: physics
    type: character
  - !ruby/object:AnimationComponent 
    animations: 
      strafe_left: /data/entities/animations/marine/marine_strafe_left.hkx
      stopped: /data/entities/animations/marine/marine_stopped.hkx
      shoot: /data/entities/animations/marine/marine_shoot.hkx
      strafe_right: /data/entities/animations/marine/marine_strafe_right.hkx
      run_backward: /data/entities/animations/marine/marine_run_backward.hkx
      bindpose: /data/entities/animations/marine/marine_bindpose.hkx
      idle: /data/entities/animations/marine/marine_idle.hkx
      run_forward: /data/entities/animations/marine/marine_run_forward.hkx
    bindPose: /data/entities/animations/marine/marine_bindpose.hkx
    defaultAnimation: stopped
    system: animation
    type: default
  - !ruby/object:SoundComponent 
    system: sound
    type: default
  - !ruby/object:ScriptComponent 
    scriptPath: /data/entities/scripts/entities/player.lua
    system: script
    type: default